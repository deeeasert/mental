        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/login_btn"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="21dp"
        android:background="@drawable/main_back"
        android:fontFamily="@font/gmarket_sans_ttf_bold"
        android:gravity="center"
        android:text="로그인"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/ai"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appCompatButton11" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/memo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="450dp"
        android:background="@drawable/main_back_2"
        android:fontFamily="@font/gmarket_sans_ttf_bold"
        android:gravity="center"
        android:text="일기"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ai"
        app:layout_constraintTop_toBottomOf="@+id/appCompatButton13" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/ai"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="36dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="70dp"
        android:background="@drawable/main_back_2"
        android:fontFamily="@font/gmarket_sans_ttf_bold"
        android:gravity="center"
        android:text="AI상담"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/log"
        app:layout_constraintEnd_toStartOf="@+id/memo"
        app:layout_constraintStart_toStartOf="parent"
